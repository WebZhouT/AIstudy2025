#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/npx" ]; then
  exec "$basedir/npx" tsx@latest "$basedir/../@flowgram.ai/form-materials/bin/index.ts" "$@"
else 
  exec npx tsx@latest "$basedir/../@flowgram.ai/form-materials/bin/index.ts" "$@"
fi
